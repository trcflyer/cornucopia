<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.easypay.cornucopiaquery.dal.dao.impl.TransOrdInfoMapperImpl" >
    <select id="selectByUniqueIndex" resultMap="com.easypay.cornucopiaquery.dal.dao.TransOrdInfoMapper.BaseResultMap"
    >
        select
        <include refid="com.easypay.cornucopiaquery.dal.dao.TransOrdInfoMapper.Base_Column_List" />
        from trans_ord_info
        where trans_date = #{transDate}
        and sqe_id = #{sqeId}
    </select>
</mapper>