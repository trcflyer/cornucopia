<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.easypay.cornucopiaallqrpay.dal.dao.impl.TMchInfoMapperImpl" >
    <select id="selectByMchId" resultMap="com.easypay.cornucopiaallqrpay.dal.dao.TMchInfoMapper.BaseResultMap"
    >
        select
        <include refid="com.easypay.cornucopiaallqrpay.dal.dao.TMchInfoMapper.Base_Column_List" />
        from t_mch_info
        where Mch_Id = #{mchId}
    </select>
    <select id="selectByMemberId" resultMap="com.easypay.cornucopiaallqrpay.dal.dao.TMchInfoMapper.BaseResultMap"
    >
        select
        <include refid="com.easypay.cornucopiaallqrpay.dal.dao.TMchInfoMapper.Base_Column_List" />
        from t_mch_info
        where member_id = #{memberId}
    </select>
</mapper>