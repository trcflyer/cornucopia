<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.easypay.cornucopiatrans.dal.dao.impl.UserLoginMapperImpl" >
    <select id="selectByLogin" resultMap="com.easypay.cornucopiatrans.dal.dao.UserLoginMapper.BaseResultMap"
    >
        select
        <include refid="com.easypay.cornucopiatrans.dal.dao.UserLoginMapper.Base_Column_List" />
        from user_login
        where login_Id = #{loginId}
        and user_State = #{userState}
    </select>
</mapper>